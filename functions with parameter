1.store:

public class store {
    void getsoap(int money){
        System.out.println(money);
        System.out.println("soap purchased");
    }
    void getchocolate(int money){
        System.out.println(money);
        System.out.println("chocolate purchased");
    }
    public static void main(String[] args) {
        store obj1 = new store();
        obj1.getsoap(20);
        obj1.getchocolate(5);
    }
    
}
